package br.com.gerenciadorstudy.servlet;

public class Empresa {
	
	//Atributos
	private int id;
	private String nome;
	
	public Empresa() {
	}
		
	public Empresa(String nome) {
		this.nome = nome;
	}
	
	//Getters e Setters
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}

	
}
